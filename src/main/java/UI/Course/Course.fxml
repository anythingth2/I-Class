<fx:root minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0"
         xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
         type="AnchorPane">
    <children>
        <Pane layoutX="342.0" layoutY="-1.0" prefHeight="53.0" prefWidth="939.0" style="-fx-background-color: #80CBC4;">
            <children>
                <Button fx:id="editButton" contentDisplay="CENTER" layoutX="859.0" layoutY="10.0"
                        mnemonicParsing="false" prefHeight="37.0" prefWidth="68.0"
                        style="-fx-background-color: #B1DFDB;" text="Edit" textAlignment="CENTER">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Button>
                <ImageView fx:id="Annoucement" fitHeight="64.09" fitWidth="71.96" layoutX="-542.0" layoutY="34.0"
                           pickOnBounds="true" preserveRatio="true" x="565.0">
                    <image>
                        <Image url="@../../img/Annoucement.png"/>
                    </image>
                </ImageView>
                <Label layoutX="105.0" prefHeight="64.0" prefWidth="837.0" text="Lorem Lorem" textFill="WHITE">
                    <font>
                        <Font name="Segoe UI Semibold" size="35.0"/>
                    </font>
                </Label>
            </children>
        </Pane>
        <Pane layoutX="343.0" layoutY="52.0" prefHeight="68.0" prefWidth="938.0" style="-fx-background-color: #E6E6E6;">
            <children>
                <Label layoutX="21.0" layoutY="-6.0" prefHeight="79.0" prefWidth="676.0" text="Title Name">
                    <font>
                        <Font name="Segoe UI Semibold" size="36.0"/>
                    </font>
                </Label>
            </children>
        </Pane>
        <VBox layoutY="5.0" prefHeight="768.0" prefWidth="342.0" style="-fx-background-color:#010101;"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Pane prefHeight="130.0" prefWidth="540.0" style="-fx-background-color: #171C1D;">
                    <children>
                        <TextFlow layoutX="1.0" prefHeight="128.0" prefWidth="340.0" textAlignment="CENTER">
                            <children>
                                <Label fx:id="subjectName" alignment="CENTER" prefHeight="128.0" prefWidth="333.0"
                                       text="Subject Name" textAlignment="CENTER" textFill="WHITE">
                                    <font>
                                        <Font name="Segoe UI Semibold" size="50.0"/>
                                    </font>
                                </Label>
                            </children>
                        </TextFlow>
                    </children>
                </Pane>
                <ScrollPane fx:id="scrollPane" prefHeight="640.0" prefWidth="286.0"
                            style="-fx-background-color: #010101; -fx-background: #010101;;">
                    <content>
                        <VBox fx:id="classVBox" prefHeight="617.0" prefWidth="324.0"
                              style="-fx-background-color: #010101;">

                        </VBox>

                    </content>
                </ScrollPane>
            </children>
        </VBox>
        <ImageView fx:id="addCourse" fitHeight="90.41" fitWidth="90.42" layoutX="405.0" layoutY="951.0"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../img/plus.png"/>
            </image>
        </ImageView>
        <ImageView fx:id="chatIcon" fitHeight="72.42" fitWidth="72.42" layoutX="1788.58" layoutY="950.58"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../img/chat.png"/>
            </image>
        </ImageView>
        <ImageView fx:id="inBoxIcon" fitHeight="72.42" fitWidth="72.42" layoutX="1695.58" layoutY="950.58"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../img/inbox.png"/>
            </image>
        </ImageView>
        <Button fx:id="button" layoutX="252.0" layoutY="652.0" mnemonicParsing="false" onMouseClicked="#onAddClick"
                prefHeight="51.0" prefWidth="68.0" text="Button">
            <effect>
                <Blend mode="BLUE"/>
            </effect>
        </Button>
        <Pane layoutX="340.0" layoutY="119.0" prefHeight="602.0" prefWidth="942.0">
            <children>
                <Circle fill="DODGERBLUE" layoutX="878.0" layoutY="543.0" radius="34.0" strokeType="INSIDE"
                        style="-fx-fill: #80CBC4;">
                    <effect>
                        <DisplacementMap>
                            <mapData>
                                <FloatMap/>
                            </mapData>
                        </DisplacementMap>
                    </effect>
                    <stroke>
                        <LinearGradient endX="1.0" endY="1.0">
                            <stops>
                                <Stop color="#0f0f0ff6"/>
                                <Stop color="WHITE" offset="1.0"/>
                            </stops>
                        </LinearGradient>
                    </stroke>
                </Circle>
            </children>
        </Pane>

    </children>
</fx:root>
